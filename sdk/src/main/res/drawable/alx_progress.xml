<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <stroke
                android:width="5dp"
                android:color="#ffffff" />
            <size
                android:width="60dp"
                android:height="60dp" />
        </shape>
    </item>

    <item>
        <shape android:shape="oval">
            <stroke
                android:width="5dp"
                android:color="#E91E63"
                android:dashWidth="100dp"
                android:dashGap="100dp" />
            <size
                android:width="60dp"
                android:height="60dp" />
        </shape>
    </item>

    <item>
        <shape android:shape="oval">
            <stroke
                android:width="5dp"
                android:color="#2196F3"
                android:dashWidth="60dp"
                android:dashGap="200dp" />
            <size
                android:width="60dp"
                android:height="60dp" />
        </shape>
    </item>

</layer-list>